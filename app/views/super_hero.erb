<form action="/teams" method="post">
  <h1>Create a Team and Heroes!</h1>
  Superhero Team Name: <input type="text" name="team[name]">
  Superhero Team Motto: <input type="text" name="team[motto]">
  Team Member 1 Name: <input type="text" name="member1_name">
  Team Member 1 Power: <input type="text" name="member1_power">
  Team Member 1 Bio: <input type="text" name="member1_bio">
  Team Member 2 Name: <input type="text" name="member2_name">
  Team Member 2 Power: <input type="text" name="member2_power">
  Team Member 2 Bio: <input type="text" name="member2_bio">
  <input type="submit" id = 'Submit'>
</form>
